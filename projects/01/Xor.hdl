/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=b, out=not1);
    And(a=a, b=not1, out=and1);

    Not(in=a, out=not2);
    And(a=not2, b=b, out=and2);

    Or(a=and1, b=and2, out=out);
}